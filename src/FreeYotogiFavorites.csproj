<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Version>1.1.0</Version>
		<Authors>Perdition</Authors>
		<PackageProjectUrl>https://github.com/Perdition-117/COM3D2.FreeYotogiFavorites</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Perdition-117/COM3D2.FreeYotogiFavorites.git</RepositoryUrl>
	</PropertyGroup>

	<PropertyGroup>
		<TargetFramework>net35</TargetFramework>
		<LangVersion>10</LangVersion>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<ItemGroup>
		<None Update="localization\*.xml">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="BepInEx.BaseLib" Version="5.4.19" />
		<PackageReference Include="BepInEx.PluginInfoProps" Version="1.1.0" />
		<PackageReference Include="COM3D2.GameLibs" Version="2.34.2-r.0" />
		<PackageReference Include="HarmonyX" Version="2.10.0" />
		<PackageReference Include="UnityEngine" Version="5.6.1" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="I2PluginLocalization">
			<HintPath>..\lib\I2PluginLocalization.dll</HintPath>
		</Reference>
	</ItemGroup>

</Project>
